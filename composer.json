{
    "name": "ttungbmt/geoserver-rest-api",
    "description": "GeoServer REST API For Laravel/PHP",
    "keywords": [
        "laravel",
        "geoserver"
    ],
    "homepage": "https://github.com/ttungbmt/geoserver-rest-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Truong Thanh Tung",
            "email": "ttungbmt@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "php-http/guzzle7-adapter": "^1.0",
        "guzzlehttp/psr7": "^2.0",
        "http-interop/http-factory-guzzle": "^1.2",
        "oneofftech/geoserver-client-php": "^0.2.0",
        "illuminate/http": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "ttungbmt\\REST\\Geoserver\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ttungbmt\\REST\\Geoserver\\GeoServerServiceProvider"
            ],
            "aliases": {
                "Geoserver": "ttungbmt\\REST\\Geoserver\\Facade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
