{
    "name": "lumbunginov/geoserver-rest-api-laravel",
    "description": "GeoServer REST API For Laravel/PHP",
    "keywords": [
        "laravel 12",
        "geoserver"
    ],
    "homepage": "https://github.com/lumbunginov/geoserver-rest-api-Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Lumbung Inovasi Digital",
            "email": "lumbunginov@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "php-http/guzzle7-adapter": "^1.0",
        "php-http/httplug": "^2.4",
        "guzzlehttp/psr7": "^2.0",
        "http-interop/http-factory-guzzle": "^1.2",
        "oneofftech/geoserver-client-php": "^0.4.0",
        "illuminate/http": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "ttungbmt\\REST\\Geoserver\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ttungbmt\\REST\\Geoserver\\GeoServerServiceProvider"
            ],
            "aliases": {
                "Geoserver": "ttungbmt\\REST\\Geoserver\\Facade"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
